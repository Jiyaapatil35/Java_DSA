import java.util.ArrayList;

public class ArrayListBasics {
    public static void main(String args[]) {
        ArrayList<Integer> list = new ArrayList<>();
        // Add operation -> ArrayList_name.add(parameter) -> O(1)
        list.add(1);
        list.add(2);
        list.add(3);
        list.add(4);
        list.add(5);
        System.out.println(list);

        // Add operation2 -> ArrayList_name.add(index, newValue) -> O(n)
        list.add(2, 9);
        System.out.println(list);

        // Get operation -> ArrayList_name.get(index) -> O(1)
        int element = list.get(2);
        System.out.println(element);

        // Delete operation -> ArrayList_name.remove(index) -> O(n)
        list.remove(3);
        System.out.println(list);

        // Set operation -> ArrayList_name.set(index, newValue) -> O(n)
        list.set(2, 10);
        System.out.println(list);

        // Contains operation -> ArrayList_name.contains(Value) -> O(n)
        System.out.println(list.contains(1));
        System.out.println(list.contains(11));

        // Size of arraylist
        System.out.println(list.size());
    }
}
